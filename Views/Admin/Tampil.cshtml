@{
    Layout="_Layout";
}
<h2>Daftar User</h2>
<a asp-action="Registrasi" class="btn btn-success btn-xs">Tambah Data</a>
<div class="row">
    @if(ViewData["pesan"]!=null){
        @Html.Raw(ViewData["pesan"])
    }
    <br/><br/>
</div>
<table class="table table-striped">
    <tr>
        <th>id</th>
        <th>UserName</th>
        <th>Password</th>
        <th></th>
        <th></th>
    </tr>
    @foreach(var data in Model){
        <tr>
            <td>@data.id_admin</td>
            <td>@data.username</td>
            <td>@data.password</td>
            <td><a asp-action="Update" asp-route-id="@data.username">
                Update</a></td>
            <td><a asp-action="Delete" asp-route-id="@data.username">delete</a></td>
        </tr>
    }
</table>